package com.example.demoSQL.service;

import java.util.ArrayList;
import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.example.demoSQL.domain.OfferDo_Y;

@Service
public class ServiceProService {
	@Autowired
	private ServiceProDao serviceProDao;
	
	public OfferDo_Y getOneService(String id) {
		return this.serviceProDao.getOne(id);
	}
	
	public List<OfferDo_Y> getAllService(){
		return this.serviceProDao.findAll();
	}
	

}
